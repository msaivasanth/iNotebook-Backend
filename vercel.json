{
    "version": 2,
    "name": "iNotebook-backend",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { 
           "src": "/(.*)", 
           "dest": "./index.js",
           "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
           "headers": {
            "Access-Control-Allow-Origin": "https://i-notebook-frontend-bice.vercel.app"
            "Access-Control-Allow-Credentials": true

         }
       }
    ]
 }
