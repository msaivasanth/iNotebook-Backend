{
    "version": 2,
    "name": "iNotebook-backend",
    "builds": [
       { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
       { 
           "src": "/(.*)", 
           "dest": "./index.js",
           // To enble cors
           "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
           }
    ]
 }
